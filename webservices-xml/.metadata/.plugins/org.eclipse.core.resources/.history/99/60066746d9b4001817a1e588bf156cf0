package info.kblogics;

public class Pattern {

	public static void main(String[] args) {
		int n = 6;

		for (int i = n; i < n; i--) {
			for (int j = 0; j =>i; j++) {

				for(int k=0;k<=n-i;k++) {
					System.out.print(" ");
				}
				System.out.print("*");
			}
			System.out.println();
		}

	}
}

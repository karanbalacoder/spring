package info.kblogics;

	
class App 
{  
	static boolean b;
	
    public static void main(String[] args) 
    { 
    	short hand = 42;
        if ( hand < 50 && !b ) {
        	 System.out.println("hii");
        }
         
        if ( 9 > 50 ) {
        	System.out.println("kkkk");
        }
        else if ( hand > 40 ) 
        {
           System.out.println("jjjj");   
        }
        else
            --hand;
        System.out.println(hand);
    	}
    	
    	
    	
        
    	
    
}
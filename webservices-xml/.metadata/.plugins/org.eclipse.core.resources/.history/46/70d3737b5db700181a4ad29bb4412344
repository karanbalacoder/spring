package info.kblogics;

	
class App 
{  
	static boolean b;
	
    public static void main(String[] args) 
    { 
    	if(1==1) {
    		System.out.println("only if");
    	}
    	
    	if(2==2) 
    		
        		System.out.println("only if");
        	
    	
    	
    	else if(3==3) {
    		System.out.println("else if");
    	}
    	
    	else if(0==4) {
    		System.out.println("else if");
    	}
         
    	else {
    		System.out.println("else");
    	}
    	
        
    	
    } 
}